
<% connections.each_with_index do |connection, index| %>
    { "property_uri": "<%=connection.property_uri%>",
      "pending": "<%=connection.pending_status%>", 
      "values": {
        "old": "<%=connection.old_value%>",
        "expected": "<%=connection.expected_value%>",
        "received": "<%=connection.received_value%>"
      } 
    }<% if (connections.length - 1 > index) %>,<%end%>
<% end %>
